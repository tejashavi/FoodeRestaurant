<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".fragment.MoreFragment">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

            <TextView
                android:id="@+id/tvTittle"
                android:layout_width="wrap_content"
                android:layout_height="?android:attr/actionBarSize"
                android:layout_centerVertical="true"
                android:layout_marginLeft="@dimen/_10sdp"
                android:gravity="center_vertical"
                android:text="Profile"
                android:textColor="@color/colorTextBlack"
                android:textSize="@dimen/_14sdp" />

            <ImageView
                android:id="@+id/ivLogout"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:layout_marginRight="@dimen/_10sdp"
                android:background="@drawable/ic_baseline_exit_to_app_24" />
        </RelativeLayout>

        <View
            android:layout_width="match_parent"
            android:layout_height="@dimen/_1sdp"
            android:background="@color/grey" />
        <androidx.core.widget.NestedScrollView
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:fillViewport="true">
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="vertical"
                android:layout_marginTop="@dimen/_10sdp"
                >

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="Name"
                    android:textSize="@dimen/_14sdp"
                    android:textColor="@color/colorTextBlack"
                    android:layout_marginHorizontal="@dimen/_10sdp"
                    />

                <TextView
                    android:id="@+id/tvName"
                    android:layout_marginTop="@dimen/_5sdp"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="Name"
                    android:layout_marginHorizontal="@dimen/_10sdp"
                    />
                <TextView
                    android:layout_marginTop="@dimen/_10sdp"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="Email"
                    android:textSize="@dimen/_14sdp"
                    android:textColor="@color/colorTextBlack"
                    android:layout_marginHorizontal="@dimen/_10sdp"
                    />

                <TextView
                    android:id="@+id/tvEmail"
                    android:layout_marginTop="@dimen/_5sdp"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="email"
                    android:layout_marginHorizontal="@dimen/_10sdp"
                    />
                <TextView
                    android:layout_marginTop="@dimen/_10sdp"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="Phone"
                    android:textSize="@dimen/_14sdp"
                    android:textColor="@color/colorTextBlack"
                    android:layout_marginHorizontal="@dimen/_10sdp"
                    />

                <TextView
                    android:id="@+id/tvPhone"
                    android:layout_marginTop="@dimen/_5sdp"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="phone"
                    android:layout_marginHorizontal="@dimen/_10sdp"
                    />
                <TextView
                    android:layout_marginTop="@dimen/_10sdp"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="Description"
                    android:textSize="@dimen/_14sdp"
                    android:textColor="@color/colorTextBlack"
                    android:layout_marginHorizontal="@dimen/_10sdp"
                    />

                <TextView
                    android:id="@+id/tvDescription"
                    android:layout_marginTop="@dimen/_5sdp"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="Description"
                    android:layout_marginHorizontal="@dimen/_10sdp"
                    />
                <TextView
                    android:layout_marginTop="@dimen/_10sdp"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="Offer Min Amount"
                    android:textSize="@dimen/_14sdp"
                    android:textColor="@color/colorTextBlack"
                    android:layout_marginHorizontal="@dimen/_10sdp"
                    />

                <TextView
                    android:id="@+id/tvOfferMinAmount"
                    android:layout_marginTop="@dimen/_5sdp"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="offer_min_amount"
                    android:layout_marginHorizontal="@dimen/_10sdp"
                    />
                <TextView
                    android:layout_marginTop="@dimen/_10sdp"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="Offer Percent"
                    android:textSize="@dimen/_14sdp"
                    android:textColor="@color/colorTextBlack"
                    android:layout_marginHorizontal="@dimen/_10sdp"
                    />

                <TextView
                    android:id="@+id/tvOfferPercent"
                    android:layout_marginTop="@dimen/_5sdp"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="Offer Percent"
                    android:layout_marginHorizontal="@dimen/_10sdp"
                    />
                <TextView
                    android:layout_marginTop="@dimen/_5sdp"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="Estimated Delivery Time"
                    android:textSize="@dimen/_14sdp"
                    android:textColor="@color/colorTextBlack"
                    android:layout_marginHorizontal="@dimen/_10sdp"
                    />

                <TextView
                    android:id="@+id/tvEstimatedDeliveryTime"
                    android:layout_marginTop="@dimen/_5sdp"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="Estimated Delivery Time"
                    android:layout_marginHorizontal="@dimen/_10sdp"
                    />
                <TextView
                    android:layout_marginTop="@dimen/_10sdp"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="Address"
                    android:textSize="@dimen/_14sdp"
                    android:textColor="@color/colorTextBlack"
                    android:layout_marginHorizontal="@dimen/_10sdp"
                    />

                <TextView
                    android:id="@+id/tvAddress"
                    android:layout_marginTop="@dimen/_5sdp"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="Address"
                    android:layout_marginHorizontal="@dimen/_10sdp"
                    />
                <TextView
                    android:layout_marginTop="@dimen/_5sdp"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="Status"
                    android:textSize="@dimen/_14sdp"
                    android:textColor="@color/colorTextBlack"
                    android:layout_marginHorizontal="@dimen/_10sdp"
                    />

                <TextView
                    android:id="@+id/tvStatus"
                    android:layout_marginTop="@dimen/_5sdp"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="status"
                    android:layout_marginHorizontal="@dimen/_10sdp"
                    />


            </LinearLayout>

        </androidx.core.widget.NestedScrollView>



    </LinearLayout>


</FrameLayout>